@model IEnumerable<EFDemo01.Models.Event>

@{
    ViewBag.Title = "ExampleList";
}

<h2>ExampleList</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EventName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EventDescription)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EventName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EventDescription)
            </td>
        </tr>
    }

</table>

<hr />

@Html.ActionLink("GO MAKE MORE EVENTS", "ExampleForm")